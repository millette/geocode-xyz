{
  "name": "geocode-xyz",
  "description":
    "Geoparse, Geocode and map your geo data to latitude,longitude and elevation.",
  "version": "0.0.0",
  "main": "index.js",
  "bin": "cli.js",
  "license": "AGPL-3.0",
  "repository": "millette/geocode-xyz",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 8.11.4"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier-standard", "git add"]
  },
  "browser": {
    "marked": false,
    "marked-terminal": false,
    "meow": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "deps": "dependency-check . && dependency-check . --unused --no-dev",
    "lint": "standard",
    "format": "prettier-standard '**/*.+(js|json|md)'",
    "test": "standard && nyc ava --serial --fail-fast",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "isomorphic-unfetch": "^2.1.1",
    "lodash.deburr": "^4.1.0",
    "marked": "^0.7.0",
    "marked-terminal": "^3.1.1",
    "meow": "^5.0.0",
    "p-throttle": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "coveralls": "^3.0.9",
    "dependency-check": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nyc": "^15.0.0",
    "prettier-standard": "^8.0.1",
    "standard": "^14.3.1"
  }
}
